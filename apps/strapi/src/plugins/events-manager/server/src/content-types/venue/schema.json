{
  "kind": "collectionType",
  "collectionName": "venues",
  "info": {
    "singularName": "venue",
    "pluralName": "venues",
    "displayName": "Venue",
    "description": "Physical venues for events - cinemas, theaters, cultural centers"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "description": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "address": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "city": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "region": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "latitude": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "longitude": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images"],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "type": {
      "type": "enumeration",
      "enum": ["cinema", "theater", "cultural-center", "museum", "other"],
      "default": "cinema",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "capacity": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "status": {
      "type": "enumeration",
      "enum": ["pending", "approved", "suspended"],
      "default": "pending",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "links": {
      "type": "component",
      "repeatable": true,
      "component": "common.link",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "cityRef": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::geography.city"
    },
    "manager": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    },
    "properties": {
      "type": "component",
      "repeatable": true,
      "component": "entity-properties.property-value",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    }
  }
}
